{
	"multipart": [
		{ "when": {"uncovered": "true"},
			"apply": {"model": "occultism:block/otherworld_leaves"}
		},
		{ "when": {"uncovered": "true"},
			"apply": [
				{"model": "occultism:block/leaves/otherworld_leaves/top0"},
				{"model": "occultism:block/leaves/otherworld_leaves/top1"},
				{"model": "occultism:block/leaves/otherworld_leaves/top2"},
				{"model": "occultism:block/leaves/otherworld_leaves/top3"}
			]
		},
		{ "when": {"uncovered": "true"},
			"apply": [
				{"model": "occultism:block/leaves/otherworld_leaves/bottom0"},
				{"model": "occultism:block/leaves/otherworld_leaves/bottom1"},
				{"model": "occultism:block/leaves/otherworld_leaves/bottom2"},
				{"model": "occultism:block/leaves/otherworld_leaves/bottom3"}
			]
		},
		{	"when": { "OR": [
				{"persistent":false, "distance":1},
				{"persistent":false, "distance":2},
				{"persistent":false, "distance":3},
				{"persistent":false, "distance":4},
				{"persistent":false, "distance":5},
				{"persistent":false, "distance":6},
				{"persistent":true},
				{"uncovered": "false"}
			]},
			"apply": {"model": "block/oak_leaves"}
		},
		{	"when": { "OR": [
				{"persistent":false, "distance":1},
				{"persistent":false, "distance":2},
				{"persistent":false, "distance":3},
				{"persistent":false, "distance":4},
				{"persistent":false, "distance":5},
				{"persistent":false, "distance":6},
				{"persistent":true},
				{"uncovered": "false"}
			]},
			"apply": [
				{"model": "block/leaves/oak_leaves/top0"},
				{"model": "block/leaves/oak_leaves/top1"},
				{"model": "block/leaves/oak_leaves/top2"},
				{"model": "block/leaves/oak_leaves/top3"}
			]
		},
		{	"when": { "OR": [
				{"persistent":false, "distance":1},
				{"persistent":false, "distance":2},
				{"persistent":false, "distance":3},
				{"persistent":false, "distance":4},
				{"persistent":false, "distance":5},
				{"persistent":false, "distance":6},
				{"persistent":true},
				{"uncovered": "false"}
			]},
			"apply": [
				{"model": "block/leaves/oak_leaves/bottom0"},
				{"model": "block/leaves/oak_leaves/bottom1"},
				{"model": "block/leaves/oak_leaves/bottom2"},
				{"model": "block/leaves/oak_leaves/bottom3"}
			]
		},
		{	"when": {"persistent":false, "distance":7, "uncovered": "false"}, 
			"apply": {"model": "block/dead_oak_leaves"}
		},
		{	"when": {"persistent":false, "distance":7, "uncovered": "false"},
			"apply": [
				{"model": "block/leaves/dead_oak_leaves/top0"},
				{"model": "block/leaves/dead_oak_leaves/top1"},
				{"model": "block/leaves/dead_oak_leaves/top2"},
				{"model": "block/leaves/dead_oak_leaves/top3"}
			]
		},
		{	"when":{"persistent":false, "distance":7, "uncovered": "false"},
			"apply": [
				{"model": "block/leaves/dead_oak_leaves/bottom0"},
				{"model": "block/leaves/dead_oak_leaves/bottom1"},
				{"model": "block/leaves/dead_oak_leaves/bottom2"},
				{"model": "block/leaves/dead_oak_leaves/bottom3"}
			]
		}
	]
}